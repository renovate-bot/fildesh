(: message_sxpb Filename .self.opt.message_sxpb)
(: message_yaml Filename .self.opt.message_yaml)

|< zec / / $(XOF message_sxpb)
|- sxpb2yaml
|- cmptxt $(XOF message_yaml)
|> stdout


